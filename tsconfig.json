        "declaration": false,
        "noResolve": false,
        "jsx": "react-native",
        "module": "commonjs",
        "target": "es2015",
        "experimentalDecorators": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "outDir": "./dist/"
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
